#include "main.h"

/**
 * print_array - prints n elements of an array of integers
 * followed by a new line
 * Numbers are separated by comma, followed by a space
 *
 * @a: given array
 * @n: number of elements to print
 *
 * Return: Nothing
 */

void print_array(int *a, int n)
{
	int i;

	for (i = 0; i < n; i++)
		printf("%d, ", a[i]);
	_putchar('\n');
}
